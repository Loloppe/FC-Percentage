{
  "$schema": "https://raw.githubusercontent.com/bsmg/BSIPA-MetadataFileSchema/master/Schema.json",
  "id": "FCPercentage",
  "name": "FCPercentage",
  "author": "ChirpyMisha",
  "version": "1.4.8",
  "description": "A Beat Saber mod to help you practice your accuracy by showing your percentage as if you had a full combo!",
  "gameVersion": "1.37.5",
  "dependsOn": {
    "BSIPA": "^4.3.5",
    "BeatSaberMarkupLanguage": "^1.12.1",
    "SiraUtil": "^3.1.12",
    "Counters+": "^2.3.10"
  },
  "loadAfter": [
    "Counters+"
  ],
  "features": {
    "CountersPlus.CustomCounter": {
      "Name": "FC Percentage",
      "CounterLocation": "FCPercentage.FCPCounter.FCPCounterController",
      "ConfigDefaults": {
        "Enabled": true,
        "Position": "BelowMultiplier",
        "Distance": 0
      },
      "BSML": {
        "Resource": "FCPercentage.FCPCounter.Configuration.BSML.CounterSettings.bsml",
        "Host": "FCPercentage.FCPCounter.Configuration.CounterConfigController",
        "Icon": "FCPercentage.Resources.Icon.png"
      }
    }
  }
}